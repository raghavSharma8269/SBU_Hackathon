    "math_requirements": {
      "calculus": {
        "option_1": ["AMS 151", "AMS 161"],
        "option_2": ["MAT 125", "MAT 126", "MAT 127"],
        "option_3": ["MAT 131", "MAT 132"],
        "option_4": ["MAT 141", "MAT 142"],
