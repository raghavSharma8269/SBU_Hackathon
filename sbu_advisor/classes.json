      {
        "code": "CSE 416",
        "name": "Software Engineering",
        "credits": 3,
        "when_taken": "Senior Spring",
        "prerequisites": ["CSE 316"],
        "sbc": "EXP+, SBS+, ESI",
        "description": "Advanced software development, project management, capstone",
        "alternative_to": "CSE 356"
      }
