      {
        "code": "CSE 214",
        "name": "Data Structures",
        "credits": 4,
        "when_taken": "Freshman Spring or Sophomore Fall",
        "prerequisites": ["CSE 114"],
        "prerequisite_for": [
          "Most upper-division CSE courses",
          "Major admission"
        ],
        "description": "Lists, stacks, queues, trees, graphs, sorting algorithms"
      }
    ],

    "required_advanced": [
      {
        "code": "CSE 215",
        "name": "Foundations of Computer Science",
        "credits": 3,
