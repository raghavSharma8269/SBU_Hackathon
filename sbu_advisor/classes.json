        "options": [
          ["BIO 201", "BIO 202", "BIO 203"],
          ["CHE 131", "CHE 132", "CHE 152"],
          ["PHY 125", "PHY 126", "PHY 127"],
          ["PHY 131", "PHY 132"],
          ["PHY 141", "PHY 142"]
        ],
        "credits": "One lecture/lab sequence (typically 7-8 credits)",
        "when_taken": "Freshman or Sophomore year"
      },
      "additional_science": {
        "credits": 3,
        "description": "One additional natural science course"
      }
    }
  },

  "major_admission_requirements": {
    "for_aoi_students": {
      "required_courses": ["CSE 113", "CSE 114", "CSE 214"],
      "minimum_grade": "B- or higher in each course",
      "minimum_gpa_in_courses": 3.2,
      "minimum_cumulative_gpa": 3.0,
      "max_repeats": 1,
      "deadline": "Must complete within one year of first CSE course",
      "application_deadlines": {
        "spring_admission": "January 5",
        "fall_admission": "June 5"
      }
    },
    "for_non_cse_students": {
      "competitive": true,
      "must_meet_aoi_requirements": true,
      "notes": "Admission not guaranteed, contingent on program capacity"
    }
  },

  "electives_requirement": {
    "upper_division_technical": {
      "count": 6,
      "minimum_credits_each": 3,
      "restrictions": [
        "Cannot double-count with required advanced courses",
        "CSE 475 (teaching practicum) does not count",
        "CSE 495 (first semester honors project) does not count",
        "Non-technical courses do not count",
        "Maximum 3 credits from CSE 487, 496, VIP courses"
      ]
    }
  }
